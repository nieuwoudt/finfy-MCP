# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Plaid Configuration
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENV=production

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# SendGrid Configuration
SENDGRID_API_KEY=your_sendgrid_api_key

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Sentry Configuration (Optional)
SENTRY_DSN=your_sentry_dsn
SENTRY_SUPPRESS_GLOBAL_ERROR_HANDLER_FILE_WARNING=1

# Yodlee Integration
YODLEE_CLIENT_ID=your_yodlee_client_id
YODLEE_CLIENT_SECRET=your_yodlee_client_secret
YODLEE_API_URL=your_yodlee_api_url
YODLEE_DATASET_NAME=your_yodlee_dataset_name
YODLEE_FASTLINK_URL=your_yodlee_fastlink_url
YODLEE_WEBHOOK_URL=your_yodlee_webhook_url

# FinGoal Integration
FINGOAL_CLIENT_ID=your_fingoal_client_id
FINGOAL_CLIENT_SECRET=your_fingoal_client_secret
FINGOAL_API_URL=your_fingoal_api_url
FINGOAL_WEBHOOK_URL=your_fingoal_webhook_url
FINGOAL_WEBHOOK_SIGNATURE=your_fingoal_webhook_signature

# Webhook Monitoring and Alert System
WEBHOOK_HEALTH_CHECK_SECRET=generate_random_secret
NEXT_PUBLIC_ENABLE_EMAIL_ALERTS=true
NEXT_PUBLIC_ENABLE_SLACK_ALERTS=false
SLACK_WEBHOOK_URL=your_slack_webhook_url
SLACK_DEFAULT_CHANNEL=webhook-alerts
SLACK_WEBHOOK_ALERTS_CHANNEL=webhook-alerts
ADMIN_EMAIL=admin@example.com
NOTIFICATION_FROM_EMAIL=alerts@finfy.ai
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key